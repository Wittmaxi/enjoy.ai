{
  "expo": {
    "name": "enjoy.ai.app",
    "owner": "tobbe-j",
    "slug": "enjoyaiapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
